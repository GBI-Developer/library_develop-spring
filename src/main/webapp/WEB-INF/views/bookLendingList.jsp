thor" var="labelAuthor" /><spring:message code="label.publisher" var="labelPublisher" /><spring:message code="label.yearPublication" var="labelYearPublication" /><spring:message code="label.registDate" var="labelRegistDate" /><body onload="load();"> <div class="container drawer drawer--left">  <jsp:include page="common/responseBookMenu.jsp" />  <jsp:include page="common/header.jsp" />  <div class="management-toggle">   <a href="/library/libraryentrance/userList?submit=${user_list}&access_user=${login}">    <button class="btn btn-primary" style="float: none;">←${labelUserList}</button>   </a>  </div>  <h4 class="head-margin"></h4>  <jsp:include page="common/bookMenu.jsp" />  <form id="frm-example" action="" method="get">   <table id="book" class="display table table-condensed" cellspacing="0" width="100%">    <thead>     <tr>      <th><!-- <input name="select_all" value="1" id="example-select-all" type="checkbox" /> --></th>      <th>${labelId}</th>      <th>${labelTitle}</th>      <th>${labelAuthor}</th>      <th>${labelPublisher}</th>      <th>${labelYearPublication}</th>      <th>${labelRegistDate}</th>     </tr>    </thead>    <tfoot id="last-tr">     <tr>      <th></th>      <th>${labelId}</th>      <th>${labelTitle}</th>      <th>${labelAuthor}</th>      <th>${labelPublisher}</th>      <th>${labelYearPublication}</th>      <th>${labelRegistDate}</th>     </tr>    </tfoot>   </table>  </form> </div> <script> // jspからjson形式の図書情報を取得 var bookInfoJson = ${tBookInfo}; var displayInfo = '${displayInfo}'; // tableのDOM要素 var table = null; $(document).ready(function (){   // テーブル日本語化   dataTablesLanguage();   // チェックボックス有りの図書一覧を作成する   table = displayBooksListHasCheckbox(bookInfoJson);   // 貸出確認ボタンを表示   chkCntIfLendBtnDisplay(table);   // 貸出確認画面遷移   CheckedLendBookInformation();   if ('${lendDecision}' != null) {     if ('${lendDecision}' == 'success') {       alert("貸出処理が完了しました。");     } else if ('${lendDecision}' == 'failed'){       alert("貸出処理が失敗しました。");     }   }   pageToggleEvnet(); }); /**  * 画面表示後実行メソッド  *  * @returns:void  */    function load() {    lendBookDisabled();    }    bookActiveDisplayChange(displayInfo);    // ハンバーガーメニュー表示    dataTablesMenu();    // デスクトップ、モバイル画面用のメニューの表示    responsive();    $(function() {      $('[data-toggle="popover"]').popover();    });  </script> <jsp:include page="common/footer.jsp"/> <jsp:include page="common/childBookEditDisplay.jsp"/></body></html>